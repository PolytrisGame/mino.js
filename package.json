{
  "name": "@qrunk/mino.js",
  "version": "1.0.0",
  "description": "GameCube controller library for the browser. Supports Nintendo, Mayflash, and Raphnet adapters via the W3C Gamepad API.",
  "main": "dist/mino.cjs.js",
  "module": "dist/mino.esm.js",
  "browser": "dist/mino.umd.js",
  "types": "dist/mino.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mino.esm.js",
      "require": "./dist/mino.cjs.js",
      "browser": "./dist/mino.umd.js",
      "types": "./dist/mino.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "node test/test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "gamecube",
    "controller",
    "gamepad",
    "nintendo",
    "mayflash",
    "raphnet",
    "input",
    "gaming",
    "smash",
    "melee"
  ],
  "author": "Mwynhad Softworks",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/PolytrisGame/mino.js"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.9.0"
  }
}
